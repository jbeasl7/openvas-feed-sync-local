                "cvss_v2": "AV:L/AC:L/Au:N/C:C/I:C/A:C",
                "cvss_v3": "CVSS:3.1/AV:L/AC:L/PR:L/UI:N/S:U/C:H/I:H/A:H"
            }
        },
