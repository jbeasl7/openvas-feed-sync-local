            "oid": "1.3.6.1.4.1.25623.1.1.9.2024.610010110098995991029",
            "title": "Fedora: Security Advisory (FEDORA-2024-6dedbc5cf9)",
            "creation_date": 1726805316,
            "last_modification": 1729514100,
            "advisory_id": "FEDORA-2024-6dedbc5cf9",
            "advisory_xref": "https://bodhi.fedoraproject.org/updates/FEDORA-2024-6dedbc5cf9",
            "cves": [
                "CVE-2015-2104",
                "CVE-2023-27043",
                "CVE-2024-28757",
                "CVE-2024-4030",
                "CVE-2024-4032",
                "CVE-2024-45490",
                "CVE-2024-45491",
